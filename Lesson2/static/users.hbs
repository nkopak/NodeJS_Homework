<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<h1>All Users</h1>
{{#each users}}
    <div style="width: 300px; background-color: burlywood; padding: 10px">
        <div>User name: {{this.name}}</div>
        <div>User email: {{this.email}}</div>
        <hr/>
    </div>
{{/each}}
</body>
</html>
